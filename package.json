{
  "name": "@docutap-api/blue-button-generate",
  "version": "0.0.0-development",
  "description": "Blue Button CCDA Generator.",
  "main": "./index.js",
  "browser": {
    "./lib/xmlutil.js": "./browser/lib/xmlutil.js"
  },
  "directories": {
    "doc": "doc",
    "lib": "lib"
  },
  "scripts": {
    "cz": "git-cz",
    "test": "yarn run lint && yarn run unit && yarn run e2e",
    "unit": "jest",
    "generate-ccds": "node ./e2e/generate-ccds.js",
    "lint.markdown": "markdownlint -c .markdownlintrc.json 'README.md'",
    "e2e": "node ./e2e/generate-ccds.js && node ./e2e/validate-files.js",
    "lint": "yarn run lint.markdown && yarn run semantic-commitlint",
    "semantic-commitlint": "semantic-commitlint",
    "semantic-release": "semantic-release"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">= 0.10.0"
  },
  "dependencies": {
    "blue-button-meta": "1.5.0",
    "blue-button-util": "1.5.0",
    "libxmljs": "0.18.7",
    "moment": "2.10.2",
    "uuid": "2.0.1"
  },
  "devDependencies": {
    "blue-button": "1.10.2",
    "commitizen": "^2.10.1",
    "condition-circle": "^2.0.1",
    "cz-customizable": "^5.2.0",
    "jest": "^23.5.0",
    "markdownlint-cli": "^0.7.1",
    "request": "^2.85.0",
    "semantic-commitlint": "^1.3.2",
    "semantic-release": "^15.1.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DocuTAP/blue-button-generate.git"
  },
  "keywords": [
    "bluebutton"
  ],
  "bugs": {
    "url": "https://github.com/docutap/blue-button-generate/issues"
  },
  "homepage": "https://github.com/docutap/blue-button-generate",
  "release": {
    "branch": "develop",
    "verifyConditions": [
      "@semantic-release/git",
      "@semantic-release/github",
      "condition-circle"
    ],
    "getLastRelease": "@semantic-release/git",
    "verifyRelease": [
      "semantic-commitlint"
    ]
  },
  "publishConfig": {
    "tag": "next",
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.js"
    }
  }
}
